<?php

namespace App;

use Illuminate\Database\Eloquent\Model;

class Suppliers extends Model {
	protected $table = 'suppliers';

	protected $primaryKey = 'supplierId';

	protected $fillable = [
		'departmentId',
		'supplierTitle',
		'yearsOfExperience',
		'reference',
		'remarks',
		'updated_at',
		'created_at'

	];

}
